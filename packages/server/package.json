{
  "name": "@cubecobra/server",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "copy-prod-client": "mkdir -p dist/server/src/client/ && cp -r ../client/dist/* dist/server/src/client/",
    "copy-dev-client": "rm -rf src/client && mkdir -p src/client/  && cp -r ../client/dist/* src/client/",
    "copy-client": "npm run copy-prod-client && npm run copy-dev-client",
    "fix-external-paths": "npx resolve-tspaths && node scripts/fixExternalPaths.js",
    "build": "npx tsc && npm run fix-external-paths && npm run copy-client && copyfiles -u 1 'src/**/*.pug' dist/server/src && copyfiles -u 1 public/**/* dist/server/public && copyfiles -u 2 'src/emails/**/*' dist/server/src/emails",
    "type-check": "npx tsc --noEmit",
    "start": "node server.js",
    "dev": "nodemon -r tsconfig-paths/register src/index.ts",
    "publish": "npm run build && node publish.js",
    "clean": "rm -rf dist/ node_modules/ private/ ./src/client",
    "test": "npx jest --config test/jest.config.ts --coverage=false",
    "test:watch": "npx jest --config test/jest.config.ts --watch --coverage=false",
    "test:coverage": "npx jest --config test/jest.config.ts --coverage",
    "test:ci": "npx jest --config test/jest.config.ts --coverage --coverageReporters json-summary text"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
